<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="ua.imshvets.calculator.MainActivity"
    android:clickable="false">


    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button1"
        android:src="@drawable/button1"
        android:layout_above="@+id/Button4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="add1" />

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button2"
        android:src="@drawable/button2"
        android:layout_alignTop="@+id/Button1"
        android:layout_toRightOf="@+id/Button1"
        android:layout_toEndOf="@+id/Button1"
        android:onClick="add2"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button3"
        android:src="@drawable/button3"
        android:layout_alignTop="@+id/Button2"
        android:layout_toRightOf="@+id/Button2"
        android:layout_toEndOf="@+id/Button2"
        android:onClick="add3"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button4"
        android:src="@drawable/button4"
        android:layout_above="@+id/Button7"
        android:layout_alignLeft="@+id/Button7"
        android:layout_alignStart="@+id/Button7"
        android:onClick="add4"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button5"
        android:layout_below="@+id/Button1"
        android:layout_toRightOf="@+id/Button4"
        android:layout_toEndOf="@+id/Button4"
        android:src="@drawable/button5"
        android:onClick="add5"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button6"
        android:layout_below="@+id/Button2"
        android:layout_toRightOf="@+id/Button5"
        android:layout_toEndOf="@+id/Button5"
        android:src="@drawable/button6"
        android:onClick="add6"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button7"
        android:src="@drawable/button7"
        android:layout_above="@+id/ButtonPoint"
        android:layout_alignLeft="@+id/ButtonPoint"
        android:layout_alignStart="@+id/ButtonPoint"
        android:onClick="add7"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button8"
        android:src="@drawable/button8"
        android:layout_below="@+id/Button4"
        android:layout_toRightOf="@+id/Button4"
        android:layout_toEndOf="@+id/Button4"
        android:onClick="add8"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button9"
        android:layout_below="@+id/Button5"
        android:layout_alignLeft="@+id/Button6"
        android:layout_alignStart="@+id/Button6"
        android:src="@drawable/button9"
        android:onClick="add9"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/ButtonPoint"
        android:src="@drawable/buttonpoint"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="addpoint"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Button0"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/Button8"
        android:layout_alignStart="@+id/Button8"
        android:src="@drawable/button0"
        android:onClick="add0"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/ButtonEq"
        android:layout_below="@+id/Button8"
        android:layout_toRightOf="@+id/Button0"
        android:layout_toEndOf="@+id/Button0"
        android:src="@drawable/buttoneq"
        android:onClick="equile"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/ButtonDel"
        android:layout_above="@+id/Button3"
        android:layout_toRightOf="@+id/Button3"
        android:layout_toEndOf="@+id/Button3"
        android:src="@drawable/buttondel"
        android:onClick="delete"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/ButtonDivide"
        android:layout_alignTop="@+id/Button3"
        android:layout_toRightOf="@+id/Button3"
        android:layout_toEndOf="@+id/Button3"
        android:src="@drawable/buttondivide"
        android:onClick="divide"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/ButtonMulti"
        android:layout_below="@+id/ButtonDivide"
        android:layout_toRightOf="@+id/Button6"
        android:layout_toEndOf="@+id/Button6"
        android:src="@drawable/buttonmulti"
        android:onClick="multipli"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/ButtonPlus"
        android:src="@drawable/buttonplus"
        android:layout_below="@+id/Button9"
        android:layout_toRightOf="@+id/Button9"
        android:layout_toEndOf="@+id/Button9"
        android:onClick="plus"/>

    <ImageButton
        android:layout_width="83dp"
        android:layout_height="90dp"
        android:id="@+id/Buttonminus"
        android:layout_below="@+id/Button6"
        android:layout_toRightOf="@+id/Button6"
        android:layout_toEndOf="@+id/Button6"
        android:src="@drawable/buttonminus"
        android:onClick="minus"/>

    <ImageButton
        android:layout_width="249dp"
        android:layout_height="90dp"
        android:id="@+id/Buttonc"
        android:layout_alignBottom="@+id/ButtonDel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/buttonc"
        android:onClick="sbros"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView"
        android:layout_above="@+id/Buttonc"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:layout_alignRight="@+id/ButtonDel"
        android:layout_alignEnd="@+id/ButtonDel" />

    <TextView
        android:layout_width="83dp"
        android:layout_height="45dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toStartOf="@+id/textView4"
        android:layout_toLeftOf="@+id/textView4" />

    <TextView
        android:layout_width="83dp"
        android:layout_height="45dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/ButtonDel"
        android:layout_alignEnd="@+id/ButtonDel"
        android:layout_toRightOf="@+id/Buttonc"
        android:layout_toEndOf="@+id/Buttonc" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/textView4"
        android:layout_above="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_alignTop="@+id/textView3" />

</RelativeLayout>
